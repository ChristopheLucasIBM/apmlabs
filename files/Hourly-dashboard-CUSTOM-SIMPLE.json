{
    "timeGrain": "hour",
    "cards": [
        {
            "id": "car-image",
            "dataSource": {
                "attributes": [
                    {
                        "attribute": "temperature",
                        "id": "temperature",
                        "aggregator": "mean"
                    }
                ],
                "range": {
                    "type": "periodToDate",
                    "count": -1,
                    "interval": "month"
                }
            },
            "content": {
                "image": "CL_Asset",
                "alt": "CL_Asset",
                "hotspots": [
                    {
                        "color": "#00F",
                        "icon": "icon--info--outline",
                        "content": {
                            "attributes": [
                                {
                                    "dataSourceId": "temperature",
                                    "label": "temperature"
                                }
                            ]
                        },
                        "locations": [
                            {
                                "x": "28",
                                "y": "62"
                            }
                        ]
                    }
                ]
            },            
            "size": "LARGE",
            "title": "Have you found the GOLDEN NUGGET yet ... ?",
            "type": "IMAGE"
        },
        {
            "id": "card-temperature-value",
            "content": {
                "attributes": [
                    {
                        "dataSourceId": "temperature_Hourly_max",
                        "precision": 3,
                        "label": "Max Temperature",
                        "thresholds": [
                            {
                                "dataSourceId": "temperature_Hourly_mean",
                                "value": 0,
                                "comparison": ">=",
                                "color": "red",
                                "icon": "icon--warning--solid"
                            },
                            {
                                "color": "red",
                                "comparison": ">",
                                "icon": "icon--checkmark--outline",
                                "value": 250
                            }
                        ]
                    },
                    {
                        "dataSourceId": "temperature_Hourly_mean",
                        "precision": 3,
                        "label": "Mean Temperature",
                        "thresholds": [
                            {
                                "color": "#5aa700",
                                "comparison": "<",
                                "icon": "icon--warning",
                                "value": 1000
                            },
                            {
                                "color": "#F00",
                                "comparison": ">",
                                "icon": "icon--warning",
                                "value": 250
                            }
                        ]
                    },
                    {
                        "dataSourceId": "temperature_Hourly_min",
                        "precision": 2,
                        "label": "Min Temperature",
                        "thresholds": [
                            {
                                "dataSourceId": "current_Wastewater_Plant_Hourly_min",
                                "value": 0,
                                "comparison": ">=",
                                "color": "orange",
                                "icon": "icon--warning--solid"
                            }
                        ]
                    }
                ]
            },
            "dataSource": {
                "attributes": [
                    {
                        "aggregator": "max",
                        "attribute": "temperature_Hourly_max",
                        "id": "temperature_Hourly_max"
                    },
                    {
                        "aggregator": "mean",
                        "attribute": "temperature_Hourly_mean",
                        "id": "temperature_Hourly_mean"
                    },
                    {
                        "aggregator": "min",
                        "attribute": "temperature_Hourly_min",
                        "id": "temperature_Hourly_min"
                    }
                ]
            },
            "size": "MEDIUMTHIN",
            "title": "Temperature",
            "type": "VALUE"
        },
        {
            "id": "card-pressure-vibration-value",
            "content": {
                "attributes": [
                    {
                        "dataSourceId": "pressure_Hourly_mean",
                        "precision": 2,
                        "label": "Mean Pressure",
                        "thresholds": [
                            {
                                "dataSourceId": "pressure_Hourly_mean",
                                "value": 0,
                                "comparison": ">=",
                                "color": "red",
                                "icon": "icon--warning--solid"
                            },
                            {
                                "color": "red",
                                "comparison": ">",
                                "icon": "icon--checkmark--outline",
                                "value": 250
                            }
                        ]
                    },
                    {
                        "dataSourceId": "vibration_Hourly_mean",
                        "precision": 3,
                        "label": "Mean Vibration",
                        "thresholds": [
                            {
                                "color": "#5aa700",
                                "comparison": "<",
                                "icon": "icon--warning",
                                "value": 1000
                            },
                            {
                                "color": "#F00",
                                "comparison": ">",
                                "icon": "icon--warning",
                                "value": 250
                            }
                        ]
                    }
                ]
            },
            "dataSource": {
                "attributes": [
                    {
                        "aggregator": "mean",
                        "attribute": "pressure_Hourly_mean",
                        "id": "pressure_Hourly_mean"
                    },
                    {
                        "aggregator": "mean",
                        "attribute": "vibration_Hourly_mean",
                        "id": "vibration_Hourly_mean"
                    }                                                                                                    
                ]
            },
            "size": "MEDIUMTHIN",
            "title": "Pressure, Vibration",
            "type": "VALUE"
        },
        {
            "id": "card-temperature-pressure-vibration-timeseries",
            "dataSource": {
                "attributes": [
                    {
                        "aggregator": "max",
                        "attribute": "temperature",
                        "id": "temperature"
                    },
                    {
                        "aggregator": "max",
                        "attribute": "pressure",
                        "id": "pressure"
                    },
                    {
                        "aggregator": "max",
                        "attribute": "vibration",
                        "id": "vibration"
                    }
                ],
                "range": {
                    "count": -120,
                    "interval": "hour"
                },
                "timeGrain": "hour"
            },
            "content": {
                "series": [
                    {
                        "dataSourceId": "temperature",
                        "label": "temperature"
                    },
                    {
                        "dataSourceId": "pressure",
                        "label": "pressure"
                    },
                    {
                        "dataSourceId": "vibration",
                        "label": "vibration"
                    }
                ],
                "xLabel": "Time",
                "yLabel": "Values"
            },
            "size": "MEDIUM",
            "title": "Max temperature, Pressure, Vibration",
            "type": "TIMESERIES"
        },
        {
            "id": "card-starts-stacked-time-barchart",
            "size": "MEDIUMWIDE",
            "title": "Starts over time",
            "type": "BAR",
            "content": {
                "layout": "VERTICAL",
                "type": "STACKED",
                "series": [
                    {
                        "dataSourceId": "starts_Hourly_count",
                        "label": "Starts",
