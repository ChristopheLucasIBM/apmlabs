                            "red"
                        ]
                    },
                    {
                        "dataSourceId": "starts_Hourly_mean",
                        "label": "Blockages",
                        "color": [
                            "green"
                        ]
                    }
                ],
                "timeDataSourceId": "timestamp",
                "xLabel": "Time",
                "yLabel": "Starts."
            },
            "dataSource": {
                "attributes": [
                    {
                        "aggregator": "mean",
                        "attribute": "starts_Hourly_count",
                        "id": "starts_Hourly_count"
                    },
                    {
                        "aggregator": "mean",
                        "attribute": "starts_Hourly_count",
                        "id": "starts_Hourly_count"
                    }
                ],
                "range": {
                    "type": "rolling",
                    "count": -1,
                    "interval": "day"
                },
                "timeGrain": "hour"
            }
        },                     
        {
            "id": "card-temperature_alert_fft",
            "size": "LARGEWIDE",
            "title": "Temperature (Fast Fourier Transform) FFT Alerts",
            "type": "ALERT",
            "dataSource": {
                "range": {
                    "count": -96,
                    "interval": "hour"
                },
                "timeGrain": "input",
                "type": "alert"
            }
        }        
