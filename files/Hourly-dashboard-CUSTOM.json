                ],
                "range": {
                    "count": -120,
                    "interval": "hour"
                },
                "additionalData": {
                    "type": "alert",
                    "dataFilter": {
                        "name": [
                            "temperature_alert_spectral",
                            "temperature_alert_generalized",
                            "temperature_alert_kmeans"
                        ]
                    }
                },
                "timeGrain": "hour"
            },
            "content": {
                "series": [
                    {
                        "dataSourceId": "temperature",
                        "label": "Temperature (Max)"
                    },
                    {
                        "dataSourceId": "temperature_anomaly_fft",
                        "label": "Anomaly (FFT)"
                    },
                    {
                        "dataSourceId": "temperature_anomaly_generalized",
                        "label": "Anomaly (Generalized)"
                    },
                    {
                        "dataSourceId": "temperature_anomaly_kmeans",
                        "label": "Anomaly (KMeans)"
                    },
                    {
                        "dataSourceId": "temperature_anomaly_saliency",
                        "label": "Anomaly (Saliency)"
                    },
                    {
                        "dataSourceId": "temperature_anomaly_spectral",
                        "label": "Anomaly (Spectral)"
                    }
                ],
                "xLabel": "Time",
                "yLabel": "Score"
            },
            "size": "LARGEWIDE",
            "title": "Temperature Anomalies & Alerts",
            "type": "TIMESERIES"
        }                               
