{
    "timeGrain": "hour",
    "cards": [
        {
            "id": "card-image",
            "dataSource": {
                "attributes": [
                    {
                        "attribute": "temperature",
                        "id": "temperature",
                        "aggregator": "mean"
                    },
                    {
                        "attribute": "temperature_anomaly_fft",
                        "id": "temperature_anomaly_fft",
                        "aggregator": "mean"
                    },
                    {
                        "attribute": "pressure",
                        "id": "pressure",
                        "aggregator": "mean"
                    },
                    {
                        "attribute": "vibration",
                        "id": "vibration",
                        "aggregator": "min"
                    },
                    {
                        "attribute": "starts_Hourly_count",
                        "id": "starts_Hourly_count",
                        "aggregator": "max"
                    }                                                             
                                                                                 
                ],
                "range": {
                    "type": "periodToDate",
                    "count": -1,
                    "interval": "month"
                }
            },
            "content": {
                "image": "CL_Asset",
                "alt": "CL_Asset",
                "hotspots": [
                    {
                        "color": "#00F",
                        "icon": "icon--info--outline",
                        "content": {
                            "title": "Simple Hotspot",
                            "description": "Just a TEMP value in the middle of the ocean. Keep looking out for the golden nugget, it's really close ...",                        
                            "attributes": [
                                {
                                    "dataSourceId": "temperature",
                                    "label": "temperature"
                                }
                            ]
                        },
                        "locations": [
                            {
                                "x": "9",
                                "y": "12"
                            }
                        ]
                    },
                    {
                        "icon": "icon--info--solid",
                        "color": "red",
                        "content": {
                            "title": "GOLDEN NUGGET found !",
                            "description": "Christophe Lucas was here in December 2020 and dropped this golden nugget. If you also made it here today, it means you completed the APM labs - CONGRATULATIONS ! With this dashboard, you are rewarded with a special view of the data items you've been working on so hard lately. Hope you enjoyed this lab, cheers, Christophe",
                            "attributes": [
                                {
                                    "dataSourceId": "temperature_anomaly_fft",
                                    "label": "TEMP Fast Fourier Transform Anomaly",
                                    "unit": "Points"
                                },
                                {
                                    "dataSourceId": "pressure",
                                    "label": "PRESSURE (MEAN)",
                                    "unit": "Points"
                                },
                                {
                                    "dataSourceId": "starts_Hourly_count",
                                    "label": "Your Asset restarted ",
                                    "unit": "times"
                                }
                            ]
                        },
                        "locations": [
                            {
                                "x": "28",
                                "y": "63"
                            }
                        ],
                        "thresholds": [
                            {
                                "dataSourceId": "water_usage",
                                "value": 0,
                                "comparison": ">=",
                                "color": "red",
                                "icon": "icon--info--solid"
                            }
                        ]
                    }                    
                ]
            },            
            "size": "LARGE",
            "title": "Have you found the GOLDEN NUGGET yet ... ?",
            "type": "IMAGE"
        },
        {
            "id": "card-temperature-value",
            "content": {
                "attributes": [
                    {
                        "dataSourceId": "temperature_Hourly_max",
                        "precision": 3,
                        "label": "Max Temperature",
                        "thresholds": [
                            {
                                "dataSourceId": "temperature_Hourly_mean",
                                "value": 0,
                                "comparison": ">=",
                                "color": "red",
                                "icon": "icon--warning--solid"
                            },
                            {
                                "color": "red",
                                "comparison": ">",
                                "icon": "icon--checkmark--outline",
                                "value": 250
                            }
                        ]
                    },
                    {
                        "dataSourceId": "temperature_Hourly_mean",
                        "precision": 3,
                        "label": "Mean Temperature",
                        "thresholds": [
                            {
                                "color": "#5aa700",
                                "comparison": "<",
                                "icon": "icon--warning",
                                "value": 1000
                            },
                            {
                                "color": "#F00",
                                "comparison": ">",
                                "icon": "icon--warning",
                                "value": 250
                            }
                        ]
                    },
                    {
                        "dataSourceId": "temperature_Hourly_min",
                        "precision": 2,
                        "label": "Min Temperature",
                        "thresholds": [
                            {
                                "dataSourceId": "current_Wastewater_Plant_Hourly_min",
                                "value": 0,
                                "comparison": ">=",
                                "color": "orange",
                                "icon": "icon--warning--solid"
                            }
                        ]
                    }
                ]
            },
            "dataSource": {
                "attributes": [
                    {
                        "aggregator": "max",
                        "attribute": "temperature_Hourly_max",
                        "id": "temperature_Hourly_max"
                    },
                    {
                        "aggregator": "mean",
                        "attribute": "temperature_Hourly_mean",
                        "id": "temperature_Hourly_mean"
                    },
                    {
                        "aggregator": "min",
                        "attribute": "temperature_Hourly_min",
                        "id": "temperature_Hourly_min"
                    }
                ]
            },
            "size": "MEDIUMTHIN",
            "title": "Temperature",
            "type": "VALUE"
        },
        {
            "id": "card-pressure-vibration-value",
            "content": {
                "attributes": [
                    {
                        "dataSourceId": "pressure_Hourly_mean",
                        "precision": 2,
                        "label": "Mean Pressure",
                        "thresholds": [
                            {
                                "dataSourceId": "pressure_Hourly_mean",
                                "value": 0,
                                "comparison": ">=",
                                "color": "red",
                                "icon": "icon--warning--solid"
                            },
                            {
                                "color": "red",
                                "comparison": ">",
                                "icon": "icon--checkmark--outline",
                                "value": 250
                            }
                        ]
                    },
                    {
                        "dataSourceId": "vibration_Hourly_mean",
                        "precision": 3,
                        "label": "Mean Vibration",
                        "thresholds": [
                            {
                                "color": "#5aa700",
                                "comparison": "<",
                                "icon": "icon--warning",
                                "value": 1000
                            },
                            {
                                "color": "#F00",
                                "comparison": ">",
                                "icon": "icon--warning",
                                "value": 250
                            }
                        ]
                    }
                ]
            },
            "dataSource": {
                "attributes": [
                    {
                        "aggregator": "mean",
                        "attribute": "pressure_Hourly_mean",
                        "id": "pressure_Hourly_mean"
                    },
                    {
                        "aggregator": "mean",
                        "attribute": "vibration_Hourly_mean",
                        "id": "vibration_Hourly_mean"
                    }                                                                                                    
                ]
            },
            "size": "MEDIUMTHIN",
            "title": "Pressure, Vibration",
            "type": "VALUE"
        },
        {
            "id": "card-temperature-pressure-vibration-timeseries",
            "dataSource": {
                "attributes": [
                    {
                        "aggregator": "max",
                        "attribute": "temperature",
                        "id": "temperature"
                    },
                    {
                        "aggregator": "max",
                        "attribute": "pressure",
                        "id": "pressure"
                    },
                    {
                        "aggregator": "max",
                        "attribute": "vibration",
                        "id": "vibration"
                    }
                ],
                "range": {
                    "count": -120,
                    "interval": "hour"
                },
                "timeGrain": "hour"
            },
            "content": {
                "series": [
                    {
                        "dataSourceId": "temperature",
                        "label": "temperature"
                    },
                    {
                        "dataSourceId": "pressure",
                        "label": "pressure"
                    },
                    {
                        "dataSourceId": "vibration",
                        "label": "vibration"
                    }
                ],
                "xLabel": "Time",
                "yLabel": "Values"
            },
            "size": "MEDIUM",
            "title": "Max temperature, Pressure, Vibration",
            "type": "TIMESERIES"
        },
        {
            "id": "card-temperature-barchart",
            "size": "MEDIUM",
            "title": "Max Temperature by City",
            "type": "BAR",
            "content": {
                "layout": "HORIZONTAL",
                "type": "SIMPLE",
                "series": [
                    {
                        "dataSourceId": "temperature"
                    }
                ],
                "categoryDataSourceId": "descriptiveLocation",
                "xLabel": "City",
                "yLabel": "Max"
            },
            "dataSource": {
                "groupBy": [
                    "descriptiveLocation"
                ],
                "attributes": [
                    {
                        "aggregator": "max",
                        "attribute": "temperature",
                        "id": "temperature"
                    }
                ],
                "range": {
                    "type": "rolling",
                    "count": -7,
                    "interval": "day"
                }
            }
        },        
        {
            "id": "card-starts-barchart",
            "size": "MEDIUM",
            "title": "Starts Hourly Count Max",
            "type": "BAR",
            "content": {
                "layout": "VERTICAL",
                "type": "SIMPLE",
                "series": [
                    {
                        "dataSourceId": "starts_Hourly_count"
                    }
                ],
                "categoryDataSourceId": "descriptiveLocation",
                "xLabel": "City",
                "yLabel": "Max"
            },
            "dataSource": {
                "groupBy": [
                    "descriptiveLocation"
                ],
                "attributes": [
                    {
                        "aggregator": "max",
                        "attribute": "starts_Hourly_count",
                        "id": "starts_Hourly_count"
                    }
                ],
                "range": {
                    "type": "rolling",
                    "count": -7,
                    "interval": "day"
                }
            }
        },        
        
        {
            "id": "card-starts-stacked-time-barchart",
            "size": "MEDIUMWIDE",
            "title": "Starts & Load over time",
            "type": "BAR",
            "content": {
                "layout": "VERTICAL",
                "type": "STACKED",
                "series": [
                    {
                        "dataSourceId": "starts_Hourly_count",
                        "label": "Starts",
                        "color": [
                            "red"
                        ]
                    },
                    {
                        "dataSourceId": "load_Hourly_mean",
                        "label": "Load",
                        "color": [
                            "green"
                        ]
                    }
                ],
                "timeDataSourceId": "timestamp",
                "xLabel": "Time",
                "yLabel": "Starts."
            },
            "dataSource": {
                "attributes": [
                    {
                        "aggregator": "mean",
                        "attribute": "starts_Hourly_count",
                        "id": "starts_Hourly_count"
                    },
                    {
                        "aggregator": "mean",
                        "attribute": "load_Hourly_mean",
                        "id": "load_Hourly_mean"
                    }
                ],
                "range": {
                    "type": "rolling",
                    "count": -1,
                    "interval": "day"
                },
                "timeGrain": "hour"
            }
        },
        {
            "id": "group-barchart",
            "size": "MEDIUMWIDE",
            "title": "Mean Vibration & Pressure by Model",
            "type": "BAR",
            "content": {
                "layout": "VERTICAL",
                "type": "GROUPED",
                "series": [
                    {
                        "dataSourceId": "vibration",
                        "label": "Vibration",
                        "color": [
                            "red"
                        ]
                    },
                    {
                        "dataSourceId": "pressure",
                        "label": "Pressure",
                        "color": [
                            "green"
                        ]
                    }
                ],
                "categoryDataSourceId": "model",
                "xLabel": "Model",
                "yLabel": "Average no."
            },
            "dataSource": {
                "groupBy": [
                    "model"
                ],
                "attributes": [
                    {
                        "aggregator": "mean",
                        "attribute": "vibration",
                        "id": "vibration"
                    },
                    {
                        "aggregator": "mean",
                        "attribute": "pressure",
                        "id": "pressure"
                    }
                ],
                "range": {
                    "type": "rolling",
                    "count": -6,
                    "interval": "day"
                },
                "timeGrain": "hour"
            }
        },                             
        {
            "id": "card-temperature_alert_fft",
            "size": "LARGEWIDE",
            "title": "Temperature (Fast Fourier Transform) FFT Alerts",
            "type": "ALERT",
